{
  "version": 2,
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/health.js"
    },
    {
      "src": "/api/users(.*)",
      "dest": "api/users.js"
    },
    {
      "src": "/api/quizzes(.*)",
      "dest": "api/quizzes.js"
    },
    {
      "src": "/api/attempts(.*)",
      "dest": "api/attempts.js"
    },
    {
      "src": "/api/results(.*)",
      "dest": "api/results.js"
    },
    {
      "src": "/api/ai(.*)",
      "dest": "api/ai.js"
    },
    {
      "src": "/api(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|xml|pdf|webp|mp4|webm|ogg|mp3|wav|flac|aac|zip|gz|tgz|bz2|rar))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(html|json))",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}